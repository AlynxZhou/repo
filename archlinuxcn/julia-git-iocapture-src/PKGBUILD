pkgname=julia-git-iocapture-src
pkgver=0.2.5
_commit=2ccb546b8a35a4fff1f3a0a81ed24e6c3a1be269
pkgrel=1
pkgdesc="IOCapture.jl"
url="https://github.com/JuliaDocs/IOCapture.jl.git"
arch=('any')
license=('MIT')
makedepends=(git julia-pkg-scripts)
depends=(julia-git)
source=("git+https://github.com/JuliaDocs/IOCapture.jl.git#commit=$_commit")
sha256sums=('SKIP')

package() {
  cd IOCapture.jl

  JULIA_INSTALL_SRCPKG=1 . /usr/lib/julia/julia-install-pkg.sh IOCapture "${pkgdir}" "${pkgname}" julia-git
}
